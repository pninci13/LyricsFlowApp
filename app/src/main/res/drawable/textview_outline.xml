<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background color (optional, can be transparent) -->
    <solid android:color="@android:color/transparent"/>

    <!-- Outline color and width -->
    <stroke
        android:width="2dp"
        android:color="#9DD69B"/> <!-- Adjust color and thickness as needed -->

    <!-- Corner radius for rounded corners -->
    <corners android:radius="10dp"/> <!-- Adjust for how rounded you want the corners -->
</shape>
